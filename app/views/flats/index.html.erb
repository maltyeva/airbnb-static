<h1>Flats#index</h1>
<p>Find me in app/views/flats/index.html.erb</p>
<% @flats.each do |flat| %>
  <p><%= flat["price"] %> - <%= flat["priceCurrency"]%></p>
  <!-- link to the show page -->
  <%= link_to flat["name"], flat_path(flat["id"]) %>
  <%= link_to flat_path(flat["id"]) do %>
    <img src="<%= flat["imageUrl"] %>" width="200">
  <% end %>
<% end %>
